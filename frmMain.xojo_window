#tag WindowCode
	#tag Event
		Sub Close()
		  App.dbClose
		  frmLog.Close
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  Clear()
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub addlistMain(str() As String)
		  
		  
		  If lstMain.ColumnCount <= str.Ubound Then
		    Dim tmp As String
		    tmp = lstMain.InitialValue
		    lstMain.ColumnCount = str.Ubound + 1
		    lstMain.InitialValue = tmp
		  End If
		  
		  lstMain.AddRow(str)
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Clear()
		  
		  frmMain.lstMain.InitialValue = ""
		  frmMain.lstMain.DeleteAllRows
		  
		End Sub
	#tag EndMethod


#tag EndWindowCode

#tag Events lstMain
	#tag Event
		Function CellClick(row as Integer, column as Integer, x as Integer, y as Integer) As Boolean
		  
		  
		  If App.WasDoubleClick Then
		    App.editMode = 1
		    App.editId = Array(CType(Val(lstMain.Cell(row, 0)), UInt64))
		    frmEdit.Show
		  End If
		  
		  
		  
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events PushButton1
	#tag Event
		Sub Action()
		  
		  App.editMode = 0
		  frmEdit.Show
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  
		  
		  //Delete
		  
		  Dim Id As String
		  
		  Id = lstMain.Cell(frmMain.lstMain.ListIndex, 0)
		  
		  App.sqlDataDelete(App.tblData, "id='" + Id + "'")
		  
		  App.showDBMain
		  
		End Sub
	#tag EndEvent
#tag EndEvents
